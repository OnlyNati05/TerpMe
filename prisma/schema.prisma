generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Page {
  id  Int @id @default(autoincrement())
  url String @unique
  status String?
  lastIndexAt DateTime?
  chunkCount Int?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}